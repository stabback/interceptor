{
  "name": "interceptor",
  "version": "2.0.0",
  "author": "Josh Stabback <josh.stabback@gmail.com>",
  "scripts": {
    "build": "vue-cli-service build --dest=./dist/client ./client/src/main.ts",
    "lint": "yarn lint:client && yarn lint:server",
    "ci": "yarn lint && yarn test",
    "clean": "rimraf ./dist",
    "dev": "ts-node bin/dev.ts",
    "dev:client": "vue-cli-service serve --port 3600 ./client/src/main.ts",
    "dev:server": "cd server && ts-node-dev --poll --respawn --transpile-only -r tsconfig-paths/register ./bin/main.ts",
    "lint:server": "eslint server --fix --ext=ts",
    "lint:client": "vue-cli-service lint ./client/src",
    "lint:client:fix": "vue-cli-service lint ./client/src --fix",
    "lint:fix": "tslint -p tsconfig.json -c tslint.json --fix",
    "start": "cross-env NODE_ENV=production && cd server && ts-node --transpile-only -r tsconfig-paths/register ./bin/main.ts",
    "test": "yarn test:server",
    "test:server": "cd server && yarn jest --run-in-band"
  },
  "dependencies": {
    "@types/concurrently": "^5.2.1",
    "@types/mongodb-memory-server": "^2.3.0",
    "@types/mongoose": "^5.7.36",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.1.0",
    "brace": "^0.11.1",
    "codemirror": "^5.57.0",
    "cookie-parser": "~1.4.4",
    "core-js": "^3.6.5",
    "debug": "~4.1.1",
    "dotenv": "^8.2.0",
    "express": "~4.17.1",
    "fast-json-patch": "^3.0.0-1",
    "http-errors": "~1.8.0",
    "http-proxy-middleware": "^0.19.2",
    "mongoose": "^5.10.0",
    "morgan": "~1.10.0",
    "object-sizeof": "^1.5.2",
    "pretty-bytes": "^5.3.0",
    "semver": "^7.0.0",
    "ts-mongoose": "^0.0.21",
    "vue": "^2.6.11",
    "vue-codemirror": "^4.0.6",
    "vue-router": "^3.2.0",
    "vue-router-multiguard": "^1.0.3",
    "vue-typed-mixins": "^0.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.0",
    "@types/http-proxy-middleware": "^0.19.2",
    "@types/jest": "^26.0.10",
    "@types/morgan": "^1.7.35",
    "@types/node": "^14.6.0",
    "@types/semver": "^7.3.3",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "chalk": "^4.1.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^26.4.0",
    "jest-junit": "^11.1.0",
    "mongodb-memory-server": "^6.6.6",
    "node-sass": "^4.12.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.3.0",
    "ts-node-dev": "1.0.0-pre.43",
    "tsconfig-paths": "^3.8.0",
    "typescript": "~3.9.3",
    "vue-template-compiler": "^2.6.11"
  },
  "license": "MIT"
}
